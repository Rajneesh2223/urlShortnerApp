# Server Environment Variables Template
# Copy this file to .env and fill in your values

# MongoDB Connection
# Option 1: Local MongoDB (default with docker-compose.yml)
# MONGO_URI=mongodb://localhost:27017/tinylink

# Option 2: MongoDB Atlas (recommended for production)
MONGO_URI=mongodb+srv://username:password@cluster0.xxxxx.mongodb.net/tinylink?retryWrites=true&w=majority

# Server Port
PORT=5000

# Node Environment
NODE_ENV=development
